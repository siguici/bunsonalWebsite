{
  "name": "host",
  "type": "module",
  "private": true,
  "scripts": {
    "astro": "bunx --bun astro",
    "sync": "bunx --bun astro sync",
    "build": "bun run fix && bunx --bun astro build",
    "check": "biome ci . && bunx --bun astro check",
    "check.format": "biome format .",
    "check.lint": "biome check .",
    "dev": "bunx --bun astro dev",
    "fix": "bun run lint && bun run format",
    "format": "biome format --write .",
    "lint": "biome check --apply-unsafe .",
    "preview": "bun run build && bun run serve",
    "prod": "bun run check && bunx --bun astro build",
    "serve": "node ./dist/server/entry.mjs",
    "start": "bunx --bun astro dev --open"
  },
  "dependencies": {
    "@astrojs/check": "^0.8.3",
    "@astrojs/mdx": "^3.1.3",
    "@astrojs/node": "^8.3.2",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/solid-js": "^4.4.0",
    "@astrojs/svelte": "^5.7.0",
    "@astrojs/tailwind": "^5.1.0",
    "@builder.io/qwik": "1.7.3",
    "@qwikdev/astro": "^0.6.2",
    "astro": "^4.12.2",
    "astro-compressor": "^0.4.1",
    "astro-robots-txt": "^1.0.0",
    "solid-js": "^1.8.19",
    "svelte": "^4.2.18",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/node": "^22.0.0",
    "postcss-import": "^16.1.0",
    "postcss-sort-media-queries": "^5.2.0"
  }
}
